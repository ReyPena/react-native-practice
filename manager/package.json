  },
  "dependencies": {
    "firebase": "^4.0.0",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "1.9.2",
    "eslint-config-rallycoding": "^3.2.0",
    "jest": "20.0.4",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
